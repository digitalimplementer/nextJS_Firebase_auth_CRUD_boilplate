{
  "hosting": {
    "public": "public",
    "site":"financia-pwa",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "nextServer"
      }
    ]
  },
  "functions": {
    "source": ".",
    "runtime": "nodejs16",
    "ignore": [
    ".firebase/**",
    ".firebaserc",
    "firebase.json",
    "**/node_modules/**",
    "**/.next/cache/**"
    ]
  }
}